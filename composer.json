{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "askedio/laravel-soft-cascade": "5.7",
        "aws/aws-sdk-php": "^3.87",
        "babenkoivan/scout-elasticsearch-driver": "^3.10",
        "barryvdh/laravel-cors": "^0.11.3",
        "barryvdh/laravel-snappy": "^0.4.6",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "~4.0",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "hashids/hashids": "^4.0",
        "intervention/image": "^2.4",
        "laravel/cashier": "^9.0",
        "laravel/framework": "5.7.*",
        "laravel/nova": "^1",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "naxon/nova-field-sortable": "^1.0",
        "nesbot/carbon": "1.*",
        "nexmo/client": "^1.6",
        "nuwave/lighthouse": "v3.0-beta.3",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.1",
        "pusher/pusher-php-server": "^3.4",
        "ramsey/uuid": "^3.8",
        "s-ichikawa/laravel-sendgrid-driver": "^2.0",
        "symfony/console": "4.*",
        "themsaid/nova-cashier-manager": "^0.2.1",
        "titasgailius/search-relations": "^1.0",
        "tymon/jwt-auth": "1.0.0-rc.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:analogrepublic/laravel-nova.git"
        }
    ]
}
